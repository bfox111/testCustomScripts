{
    "tests": [
       {
          "testName": "ELINE",
          "testDescription": "Setup ELINE between PE-6x and PE-10x.  Run Y1564 test.",
          "testType": "ELINE",

          "customScripts": [
             {
                "deviceName": "PE-6x",
                "cmdFile": "ELINE",
                "scriptAttributes": {
                  "UNI_PORT": "5",
                  "SVLAN": "610",
                  "MBPS_CIR": "1000",
                  "MD_LEVEL": "5",
                  "CIRCUIT_NAME": "ELINE610",
                  "NNI_PORT": "1",
                  "LOCAL_MEP": "1",
                  "REMOTE_MEP": "2"
                }
              },
              {
                "deviceName": "PE-10x",
                "cmdFile": "ELINE",
                "scriptAttributes": {
                  "UNI_PORT": "5",
                  "SVLAN": "610",
                  "MBPS_CIR": "1000",
                  "CIRCUIT_NAME": "ELINE610",
                  "NNI_PORT": "1",
                  "MD_LEVEL": "5",
                  "LOCAL_MEP": "2",
                  "REMOTE_MEP": "1"
                }
              }
          ],
          "sleepTime": 5,
          "verificationCommands": {
              "PE-6x": [
                  "cfm show delay",
                  "cfm show synthetic"
              ],
              "PE-10x": [
                  "show cfm delay",
                  "sh cfm synthetic-loss-measurement",
                  "show run config section fds"
              ]
          }
      },
        {
          "testName": "Y1564 Test",
          "testDescription": "Setup Y.1564 test between PE-6x and PE-10x.  PE-10x is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-10x",
                "cmdFile": "ReflectTaggedAndUntagged",
                "scriptAttributes": {
                  "UNI_PORT": "5",
                  "FLOW_POINT": "FP_5_ELINE610"
                }
              },
              {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564SetupOnceUntagged",
                "scriptAttributes": {
                  "UNI_PORT": "5",
                  "BANDWIDTH_MBS": "1000",
                  "DEST_NODE": "PE-10x",
                  "DEST_MAC": "SET_IN_CODE",
                  "FLOW_PT_GEN_PORT": "FP_5_ELINE610"
                }
              }
          ],
          "sleepTime": 5,
          "verificationCommands": {
               "PE-6x": [
                  "config search string bench"
              ],
              "PE-10x": [
                  "show run config section sat"
              ]
          }
      },
              {
          "testName": "Start Y1564 Test",
          "testDescription": "Start Y.1564 test between PE-6x and PE-10x.  PE-10x is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564StartUntagged",
                "scriptAttributes": {
              }
              }
          ],
          "sleepTime": 100,
          "verificationCommands": {
              "PE-10x": [
                  "sh sat test-instances statistics"
              ],
              "PE-6x": [
                  "bench test show name genTestUntaggedY1564 statistics"
              ]
          }
      },
      {
          "testName": "Disable Y1564 ",
          "testDescription": "Disable Y.1564 test between CE-1 and CE-2.  CE-2 is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-10x",
                "cmdFile": "DisableReflector",
                "scriptAttributes": {
                  "UNI_PORT": "5"
              }
              },
                            {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564DisableUntagged",
                "scriptAttributes": {
                  "UNI_PORT": "5"
              }
              }

          ],
          "sleepTime": 1,
          "verificationCommands": {
              "PE-10x": [
                  "show run config section sat"
              ],
              "PE-6x": [
                  "config search string bench"
              ]
          }
      },
      {
          "testName": "Delete Y1564 ",
          "testDescription": "Delete Y.1564 test between CE-1 and CE-2.  CE-2 is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-10x",
                "cmdFile": "RemoveReflector",
                "scriptAttributes": {
              }
              },
                            {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564DeleteUntagged",
                "scriptAttributes": {
              }
              }

          ],
          "sleepTime": 1,
          "verificationCommands": {
              "PE-10x": [
                  "show run config section sat"
              ],
              "PE-6x": [
                  "config search string bench"
              ]
          }
      }
    ]
 }
